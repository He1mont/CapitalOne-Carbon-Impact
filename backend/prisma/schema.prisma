generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model UserGoals {
  id    Int    @id @default(autoincrement())
  accountID Int
  goal  Int
}
model Transaction {
  id        Int     @id @default(autoincrement())
  transactionUUID String @unique
  accountID     Int
  carbonScore Float 
}
model Friends{
  id        Int     @id @default(autoincrement())
  username String
  accountID Int

}

